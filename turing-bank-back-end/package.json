{
  "name": "turing-bank-back-end",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build && npm run seed",
    "start:prod": "node dist/main.js",
    "prestart:azure": "rimraf dist && npm run build",
    "start:azure": "node dist/main.js",
    "seed": "node seeders/seeder",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/platform-fastify": "^6.5.2",
    "@nestjs/swagger": "^3.0.2",
    "@types/bcrypt": "^3.0.0",
    "bcrypt": "^3.0.6",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "fastify": "^2.6.0",
    "fastify-compress": "^0.10.0",
    "fastify-cors": "^2.1.3",
    "fastify-helmet": "^3.0.1",
    "fastify-swagger": "^2.4.0",
    "helmet": "^3.18.0",
    "moment": "^2.24.0",
    "mongoose": "^5.6.3",
    "mongoose-easy-auto-increment": "^1.0.1",
    "mongoose-paginate": "^5.0.3",
    "mongoose-paginate-v2": "^1.3.0",
    "mongoose-seed": "^0.6.0",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "randomatic": "^3.1.1",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rotating-file-stream": "^1.4.2",
    "rxjs": "^6.3.3",
    "swagger-ui-express": "^4.0.7"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/supertest": "^2.0.7",
    "concurrently": "^4.1.0",
    "jest": "^29.3.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.10.1",
    "tslint": "5.16.0",
    "typescript": "3.4.3",
    "wait-on": "^3.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
